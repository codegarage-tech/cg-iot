<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- For typeface textview -->
    <declare-styleable name="TypefacedTextView">
        <attr name="typeface" format="string" />
    </declare-styleable>

    <declare-styleable name="HangerLayout">
        <attr name="bottomDragVisibleHeight" format="dimension" />
        <attr name="bototmExtraIndicatorHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="PopUpSeekBar">
        <attr name="fsb_color_line" format="reference|color" />
        <attr name="fsb_color_ball" format="reference|color" />
        <attr name="fsb_color_ball_stroke" format="reference|color" />
        <attr name="fsb_color_value_selected" format="reference|color" />
        <attr name="fsb_value_min" format="float" />
        <attr name="fsb_value_max" format="float" />
        <attr name="fsb_value_selected" format="float" />
        <attr name="fsb_unit" format="reference|string" />
    </declare-styleable>
</resources>